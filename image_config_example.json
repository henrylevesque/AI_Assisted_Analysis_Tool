{
  "_notes": "Example JSON config for image_analysis.py (updated: within/between-model consensus naming).",
  "use_multiple_models": true,
  "models": ["gemma3:12b", "llava:13b", "llama3.2-vision:11b"],
  "type_of_analysis": "objects",
  "input": "./data/images",
  "output": "./data/image_output/results.xlsx",
  "runs": 10,
  "within_model_consensus": true,
  "within_model_consensus_mode": "fuzzy",
  "within_model_fuzzy_threshold": 85,
  "between_model_consensus": false,
  "between_model_consensus_mode": "exact",
  "between_model_fuzzy_threshold": 85,
  "delay": 0.5,
  "append_metadata": true
}